<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <link href="{{ asset('css/style.css') }}" rel="stylesheet">
        {% endblock %}
        
        
        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
    {% block navigation %}
    <div class="header">
            <a href="#">Accueil</a>
            <a href="#">Mon parcours</a>
            <a href="#">Mes productions</a>
    </div> 
    {% endblock %}
    {% block body %}{% endblock %}
    {% block footer %}
        <div class="footer">
            <div class ="footer_nav">
                <a href=""><img class ="footer_logo_pro" src="{{ asset('assets/images/linkedin-logo.png') }}" alt="Linkedin"></a>
                <a href=""><img class ="footer_logo_pro" src="{{ asset('assets/images/github-logo.png') }}" alt="Github"></a>
                <a href=""><img class ="footer_logo_pro" src="{{ asset('assets/images/youtube-logo.png') }}" alt="YouTube"></a>
            </div>
        </div>
    {% endblock %}
    </body>
</html>
